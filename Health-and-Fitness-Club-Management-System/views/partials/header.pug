head
	style
		include ../css/Navbar.css
body
	div#header
		a#link(href="/") HOME
		if (session.loggedin)
			a#llink(href="/Logout") LOG OUT
			b
			if (session.type == 1)
				a#llink(href="/Trainers") TRAINERS
				b
				a#llink(href="/Members") MEMBERS
				b
				a#llink(href="/MembersAdminView") BILLING AND PAYMENTS
				b
			else if (session.type == 2)
				a#llink(href="/Profile/" + session.user_id) Hi #{session.username}!
				b
				a#llink(href="/BillingsAndPayments") BILLING AND PAYMENTS
				b
			else if (session.type == 3)
				a#llink(href="/Profile/" + session.user_id) Hi #{session.username}!
				b
				a#llink(href="/Schedule/") SCHEDULE!
				b
				a#llink(href="/MembersTrainersView") MEMBERS
				b
		else
			a#llink(href="/Login") LOG IN
			b